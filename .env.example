# Portfolio Contribution Graph (found in settings -> developer settings -> personal access tokens)
GITHUB_ACCESS_TOKEN=your_api_key

# Resend API Key for contact form (https://resend.com)
RESEND_API_KEY=your_resend_api_key

# Sender email address (use onboarding@resend.dev for testing, or your verified domain email for production)
RESEND_FROM_EMAIL=Contact Form <onboarding@resend.dev>

# Email address to receive contact form submissions
CONTACT_EMAIL=your_email@example.com